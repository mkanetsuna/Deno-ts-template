{
  "name": "Deno Container",
  "dockerFile": "../Dockerfile",
  "customizations": {
    "vscode": {
      "extensions": [
        "denoland.vscode-deno",
        "ms-azuretools.vscode-docker"
      ],
      "settings": {
        "deno.enable": true,
        "deno.unstable": true
      }
    }
  },
  "postCreateCommand": "deno cache src/main.ts"
}
